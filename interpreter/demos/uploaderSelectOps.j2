<body>
    <div class="row">
    <div class="column">
      <br/>
        <div id="content-wrapper" class="mui--text-center">
            <form action="" method="POST" enctype="multipart/form-data">
              <h4>Server Side, "Select" TensorFlow Operations:</h4>
              <br/>
                <p><input type="file" name="file"></p>
                <br/>
                  <h6>This page classifies audio using the Tensorflow "Select Ops" Library.</h6>
                  <h6>...Upload a <pre>.mp3</pre> or a <pre>.wav / .WAV</pre> to classify:</h6>
                  <br/>
                  <p><input type="submit" value="Classify!"></p>
            </form>
            </div>
     </div>
    <div class="column">
        {% with messages = get_flashed_messages() %}
            {% if messages %}
                <ul class=flashes>
                    {% for message in messages %}
                       <li ><h6>{{ message }}</h6></li>
                    {% endfor %}
                </ul>
            {% endif %}
        {% endwith %}
    </div>
  </div>
</body>

